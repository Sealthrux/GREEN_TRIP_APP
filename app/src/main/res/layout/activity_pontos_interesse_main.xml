<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pontos_Interesse_Main">

    <com.google.android.material.chip.Chip
        android:id="@+id/ChipHoteis"
        android:layout_width="97dp"
        android:layout_height="49dp"
        android:elevation="2dp"
        android:fontFamily="sans-serif-thin"
        android:text="@string/h_teis"
        android:textSize="14sp"
        android:typeface="normal"
        app:chipIcon="@drawable/hoteis"
        app:chipIconSize="16dp"
        app:rippleColor="#FFFFFF"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="207dp" />

    <TextView
        android:id="@+id/PontosDeInteresse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="105dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="@string/pontos_de_interesse"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/pontosdeinteresseICON"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        app:layout_constraintBottom_toBottomOf="@+id/PontosDeInteresse"
        app:layout_constraintStart_toEndOf="@+id/PontosDeInteresse"
        app:layout_constraintTop_toTopOf="@+id/PontosDeInteresse"
        app:srcCompat="@drawable/pontosinteresseexeimg" />

    <ImageButton
        android:id="@+id/TopBarButton"
        style="@android:style/Widget.Holo.ImageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/topbargreen"
        tools:ignore="SpeakableTextPresentCheck"
        android:contentDescription="@string/todo" />

    <TextView
        android:id="@+id/MyGreenTripTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/montserrat_bold_italic"
        android:text="@string/my_green_trip"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/TopBarButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/HomeButtonTop"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/TopBarButton"
        app:layout_constraintEnd_toEndOf="@+id/TopBarButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/homebutton"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/FiltrosBoxSelector"
        android:layout_width="350sp"
        android:layout_height="205sp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="204dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/filtrosbox"
        android:elevation="1dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.chip.Chip
        android:id="@+id/ChipRest"
        android:layout_width="124dp"
        android:layout_height="47dp"
        android:elevation="2dp"
        android:fontFamily="@font/montserrat_light"
        android:text="@string/restaura_o"
        android:textSize="14sp"
        android:typeface="normal"
        app:chipIcon="@drawable/restauracao"
        app:chipIconSize="16dp"
        app:rippleColor="#FFFFFF"
        tools:layout_editor_absoluteX="183dp"
        tools:layout_editor_absoluteY="207dp" />

</androidx.constraintlayout.widget.ConstraintLayout>