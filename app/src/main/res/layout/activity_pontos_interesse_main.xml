<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pontos_Interesse_Main">

    <TextView
        android:id="@+id/PontosDeInteresse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="105dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="@string/pontos_de_interesse"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/pontosdeinteresseICON"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        app:layout_constraintBottom_toBottomOf="@+id/PontosDeInteresse"
        app:layout_constraintStart_toEndOf="@+id/PontosDeInteresse"
        app:layout_constraintTop_toTopOf="@+id/PontosDeInteresse"
        app:srcCompat="@drawable/pontosinteresseexeimg" />

</androidx.constraintlayout.widget.ConstraintLayout>